{
    "name": "anthonyvasiliuk/multilingual-nova",
    "description": "ðŸ’ª Multilingual Nova (using default nova fields)",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "spatie/laravel-translatable": "^6.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "autoload": {
        "psr-4": {
            "Anthonyvasiliuk\\MultilingualNova\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anthonyvasiliuk\\MultilingualNova\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anthonyvasiliuk\\MultilingualNova\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.5.3",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "brianium/paratest": "^7.0",
        "phpunit/phpunit": "^10.0",
        "laravel/framework": "^10.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
